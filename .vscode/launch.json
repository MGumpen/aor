{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch AOR (F5)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/AOR/bin/Debug/net9.0/AOR.dll",
            "args": [],
            "cwd": "${workspaceFolder}/AOR",
            "console": "internalConsole",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
                "uriFormat": "%s"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "http://localhost:5242"
            }
        }
    ]
}